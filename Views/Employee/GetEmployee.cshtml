@{
}
@model List<EmployeeReportViewModel>
<h1>Employees</h1>
<table id="PlayerTable">
    <tr>
        <th> Employee Name</th>
        <th>Employee City</th>
        <th>Employee Status</th>

    </tr>
    @foreach (var employee in Model)
    {
        
        
            <tr>
                <td>@employee.EmpName</td>
                <td>@employee.EmpCity</td>
                <td>@employee.StatusName</td>




            </tr>
        
    }
</table>


